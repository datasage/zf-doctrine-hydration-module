{
  "name": "phpro/zf-doctrine-hydration-module",
  "license": "MIT",
  "description": "Doctrine hydrators for ZF2 and ZF3",
  "scripts": {
    "test": "phpunit",
    "cs": "phpcs",
    "psalm": "psalm --show-info=false"
  },
  "keywords": [
    "laminas",
    "doctrine",
    "hydrator"
  ],
  "authors": [
    {
      "name": "Toon Verwerft",
      "email": "toon.verwerft@phpro.be"
    }
  ],
  "extra": {
    "zf": {
      "module": "Phpro\\DoctrineHydrationModule"
    }
  },
  "require": {
    "php": "^7.3",
    "doctrine/common": "^2.6.1 || ^3.0.0",
    "doctrine/doctrine-module": "^3.0.1 || ^4.0 || ^4.1",
    "doctrine/instantiator": "^1.0.5",
    "laminas/laminas-servicemanager": "^3.3.2",
    "laminas/laminas-hydrator": "^2.2.1 || ^3.2.0 || ^4.0.0",
    "laminas/laminas-modulemanager": "^2.7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "doctrine/doctrine-orm-module": "^3.0",
    "doctrine/doctrine-mongo-odm-module": "^2.0 || ^3.0",
    "doctrine/mongodb-odm": "^2.1",
    "vimeo/psalm": "^4.1",
    "psalm/plugin-phpunit": "^0.13.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Phpro\\DoctrineHydrationModule\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhproTest\\DoctrineHydrationModule\\": "test/src/"
    }
  }
}
